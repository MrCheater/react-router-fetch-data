{
  "name": "reventex",
  "version": "0.0.3",
  "description": "A Functional Reactive Game Engine",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "prettier": "prettier-eslint ./client/**/*.js ./common/**/*.js ./server/**/*.js ./tests/**/*.js --write --trailing-comma=all --single-quote --semi --bracket-spacing --tab-width=4",
    "prettier:check": "if [ \"$(prettier-eslint ./client/**/*.js ./common/**/*.js ./server/**/*.js ./tests/**/*.js --list-different --trailing-comma=all --single-quote --semi --bracket-spacing --tab-width=4)\" ]; then exit 1; fi"
  },
  "pre-commit": [
    "prettier",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reventex/reventex.git"
  },
  "keywords": [
    "game",
    "engine",
    "reactive",
    "functional",
    "game-engine",
    "gamedev",
    "game-development",
    "javascript",
    "event-driven"
  ],
  "author": "Mr.Cheater (Anton Zhukov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reventex/reventex/issues"
  },
  "homepage": "https://github.com/reventex/reventex#readme",
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.14.0",
    "jest": "^22.0.4",
    "pre-commit": "^1.2.2",
    "prettier-eslint-cli": "^4.7.0",
    "sinon": "^4.1.3"
  },
  "dependencies": {
    "co-body": "^5.1.1",
    "koa": "^2.4.1",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "nats": "^0.7.24",
    "pixi.js": "^4.6.2",
    "uuid": "^3.1.0"
  }
}
