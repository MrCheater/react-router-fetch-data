{
  "name": "reventex",
  "version": "0.0.3",
  "description": "Full stack Event Sourcing, DDD framework for Node.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "prettier": "prettier ./src/**/*.ts --write --trailing-comma=all --single-quote --semi --bracket-spacing --tab-width=2",
    "prettier:check": "if [ \"$(prettier ./src/**/*.ts --list-different --trailing-comma=all --single-quote --semi --bracket-spacing --tab-width=2)\" ]; then exit 1; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reventex/reventex.git"
  },
  "keywords": [
    "eventsourcing",
    "reactive",
    "functional",
    "typescript",
    "ddd",
    "es"
  ],
  "author": "Mr.Cheater (Anton Zhukov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reventex/reventex/issues"
  },
  "homepage": "https://github.com/reventex/reventex#readme",
  "dependencies": {
    "fp-ts": "2.10.0",
    "io-ts": "2.2.16"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@yarnpkg/pnpify": "^3.0.0-rc.3",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "mongodb": "^4.0.0-beta.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "mongodb": "^4.0.0-beta.3"
  }
}
